<h1>Listing partner_website_visits</h1>

<table>
  <thead>
    <tr>
      <th>Partner website</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partner_website_visits.each do |partner_website_visit| %>
      <tr>
        <td><%= partner_website_visit.partner_website %></td>
        <td><%= partner_website_visit.user %></td>
        <td><%= link_to 'Show', partner_website_visit %></td>
        <td><%= link_to 'Edit', edit_partner_website_visit_path(partner_website_visit) %></td>
        <td><%= link_to 'Destroy', partner_website_visit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Partner website visit', new_partner_website_visit_path %>
