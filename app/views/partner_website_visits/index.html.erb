<%- model_class = PartnerWebsiteVisits -%>
<div class="page-header">
  <h1><%=t '.title', :default => PartnerWebsiteVisits %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @partner_website_visits.each do |partner_website_visits| %>
      <tr>
        <td><%= partner_website_visits.id %></td>
        <td><%=l partner_website_visits.created_at, format: :long %></td>
        <td>
          <%= link_to partner_website_visits_path(partner_website_visits), :class => 'btn btn-xs', :title => "#{ t('.show', :default => t('helpers.links.show')) }" do %>
            <%= glyph 'info-sign' %>
            <%= glyph 'pencil' %>
          <%- end -%>
          <%= link_to partner_website_visits_path(partner_website_visits), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-xs', :title => "#{ t('.destroy', :default => t('helpers.links.destroy')) }" do %>
            <%= glyph 'remove' %>
          <%- end -%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
