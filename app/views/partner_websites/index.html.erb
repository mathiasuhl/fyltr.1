<h1>Listing partner_websites</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Token</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partner_websites.each do |partner_website| %>
      <tr>
        <td><%= partner_website.url %></td>
        <td><%= partner_website.token %></td>
        <td><%= partner_website.user %></td>
        <td><%= link_to 'Show', partner_website %></td>
        <td><%= link_to 'Edit', edit_partner_website_path(partner_website) %></td>
        <td><%= link_to 'Destroy', partner_website, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Partner website', new_partner_website_path %>
