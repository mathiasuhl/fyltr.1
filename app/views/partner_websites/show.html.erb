<%- model_class = PartnerWebsite -%>
<div class="page-header">
  <h1><%=t '.title', :default => PartnerWebsite %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:url) %></dt>
      <dd><pre class="prettyprint"><%= @partner_website.url %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:token) %></dt>
      <dd><pre class="prettyprint"><%= @partner_website.token %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              partner_websites_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_partner_website_path(@partner_website), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              partner_website_path(@partner_website),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

